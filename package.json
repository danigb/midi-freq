{
  "name": "midi.freq",
  "version": "1.0.1",
  "description": "Get frequency of a midi note",
  "main": "index.js",
  "scripts": {
    "build": "npm test && npm run docs && npm run coverage && npm run disc && npm run dist",
    "test": "vows test/*.js",
    "docs": "documentation -f md > API.md",
    "dist": "browserify index.js | uglifyjs > dist/midi.freq.min.js&& ls -hall dist/",
    "disc": "browserify --full-paths index.js | uglifyjs | discify > tmp/disc.html",
    "coverage": "istanbul cover vows test/*.js",
    "cover": "npm run coverage && open ./coverage/lcov-report/index.html",
    "climate": "npm run coverage && npm run codeclimate",
    "codeclimate": "CODECLIMATE_REPO_TOKEN= codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danigb/midi.freq.git"
  },
  "keywords": [
    "midi",
    "frequency",
    "freq",
    "music",
    "audio",
    "tonal"
  ],
  "author": "danigb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danigb/midi.freq/issues"
  },
  "homepage": "https://github.com/danigb/midi.freq#readme",
  "devDependencies": {
    "vows": "^0.8.1"
  }
}
